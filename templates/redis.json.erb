[
  <% @servers.each_with_index do |server, i| -%>
    {
      "host": "<%= server['host'] %>",
      "port": <%= server['port'] || 6379 %>,
      <% if server['password'] -%>
      "password": "<%= server['password'] %>",
      <% end -%>
      <% if server['tags'] -%>
      "tags": <%= server['tags'].to_json %>,
      <% end -%>
      "prefix": "<%= server['prefix'] || '' %>"
    }
    <% if @servers.length > 1 && i != (@servers.length - 1) -%>
      ,
    <% end -%>
  <% end -%>
]
